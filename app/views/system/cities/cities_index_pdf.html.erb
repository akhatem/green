<table class="table mt-2">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">City</th>
            <th scope="col">Created_at</th>
        </tr>
    </thead>
    <tbody>
        <% @cities.each do |city| %>
        <tr>
            <th scope="row">
                <%= city.id %>
            </th>
            <td><%= city.name %></td>
            <td><%= city.created_at.strftime("%I:%M %p %a, %d %b %Y") %></td>
        </tr>
    <% end %>
    </tbody>
</table>